/**app.wxss**/
@import "./style/weui.wxss";
@import "./style/common.wxss";

.weui-btn+.weui-btn {
  margin-top: 0px;
}

.weui-cell:before {
  content: " ";
  position: absolute;
  /* left: 0; */
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #f2f2f2;
  color: #f2f2f2;
  left: 15px;
}

.weui-navbar__item .weui-bar__item_on {
  color: #43c6ee;
}

.container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}